                        <span class="input-group-addon borderMod">Phone</span>
                    </div>
                    <div class="col-xs-7 pull-left">
                        <input class="form-control" id="txtSecondaryContactPhone" type="text" disabled>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-7">
                    <div class="col-xs-5">
                        <span class="input-group-addon borderMod">Representative Name</span>
                    </div>
                    <div class="col-xs-7">
                        <input class="form-control" id="txtRepresentativeContactName" type="text" disabled>
                    </div>
                </div>
                <div class="col-sm-5 setFloat">
                    <div class="col-xs-5">
                        <span class="input-group-addon borderMod">Phone</span>
                    </div>
                    <div class="col-xs-7 pull-left">
                        <input class="form-control" id="txtRepresentativeContactPhone" type="text" disabled>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12 well modWell">
            <div class="col-md-12">EMPLOYEE INFORMATION</div>
        </div>
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon">Total Number Of Employee:</span>
                        <input class="form-control" id="txtTinNo" type="text">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <table id="example" class="table table-striped table-bordered table-hover" width="100%;" cellspacing="0">
                        <thead>
                            <tr>
                                <th style="width: 25%">Experience Area</th>
                                <th style="width: 25%">Number</th>
                                <th style="width: 45%">Remarks</th>
                                <th style="width: 5%"></th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>Experience Area</th>
                                <th>Number</th>
                                <th>Remarks</th>
                                <th></th>
                            </tr>
                        </tfoot>
                        <tbody id="EmpInfo">
                            <tr>
                                @* HERE *@
                                <td><input type="text" class="form-control txtEmpExpArea" /></td>
                                <td><input type="text" class="form-control txtEmpNumber" /></td>
                                <td><input type="text" class="form-control txtEmpRemarks" /></td>
                                <td><input type="button" class="btn btn-primary btnEmpAdd" value="ADD MORE" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="well col-md-12">
            <div class="col-md-12"><h4>SECTION - 2 : FINANCIAL INFORMATION</h4></div>
        </div>
    </div>
    <div class="row">
