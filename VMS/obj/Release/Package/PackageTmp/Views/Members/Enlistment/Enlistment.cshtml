
@{
    ViewBag.Title = "Enlistment";
    Layout = "~/Views/Members/Shared/_Layout.cshtml";
}
<style>
    input {
        max-width: none !important;
    }

        input:disabled {
            background-color: #f9f9f9 !important;
        }

    .ValidationOK {
        color: red;
    }

    .modWell {
        padding: 2px !important;
        margin-bottom: 1% !important;
        margin-top: 2% !important;
        padding-top: 4px !important;
        padding-bottom: 4px !important;
    }

    .borderMod {
        border: 1px solid #c0c0c0 !important;
    }

    .setFloat {
        float: right !important;
    }

    .ms-parent {
        width: 100% !important;
    }

    .vcenter {
        display: inline-block;
        vertical-align: middle;
        float: none;
    }
    .TextCenter {
        text-align:center;
    }
</style>
<h2>SUPPLIER ENLISTMENT FORM</h2>

<form id="form" method="post" enctype="multipart/form-data">
    <div class="row">
        <div class="well col-md-12">
            <div class="col-md-12"><h4>SECTION - 1 : COMPANY INFORMATION</h4></div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">ORGANIZATION NAME</span>
                <input class="form-control" id="txtOrgName" type="text" disabled>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Trade License / Gov. Reg. No.:</span>
                <input class="form-control" id="txtTradeLicense" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTradeLicense">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">TIN No.:</span>
                <input class="form-control" id="txtTinNo" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTinNo">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Tel No:</span>
                <input class="form-control" id="txtTinNo" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTinNo">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Cell No.:</span>
                <input class="form-control" id="txtTinNo" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTinNo">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Fax No.:</span>
                <input class="form-control" id="txtTinNo" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTinNo">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Email No.:</span>
                <input class="form-control" id="txtTinNo" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTinNo">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <p>
                <div class="input-group">
                    <div class="col-xs-2" style="padding-left: 0; padding-right: 0; ">
                        <span class="input-group-addon borderMod" style="white-space: pre-wrap;">Company Type</span>
                    </div>
                    <div class="col-xs-3" style="padding-left: 0">
                        <select id="ddlOrgType" multiple="multiple" style="width:100% !important">
                            <option value="manufacturer">&nbsp;Corporate/Limited</option>
                            <option value="trader">&nbsp;Government</option>
                            <option value="serviceprovider">&nbsp;Partnership</option>
                            <option value="serviceprovider">&nbsp;NGO/Non-Profit</option>
                        </select>
                    </div>
                    <div class="col-xs-2" style="text-align:right; padding-right:0;">
                        <span class="" id="orgName">OTHER TYPE</span>
                    </div>
                    <div class="col-xs-3">
                        <input class="col-sm-4 form-control" id="txtOrgType" type="text" />
                    </div>
                    <div class="col-xs-2" style="padding-left: 0px; text-align:left;">
                        <button id="bTypeAdd" class="btn btn-primary" onclick="return false;">&nbsp;Add</button>
                    </div>
                    <span class="input-group-addon alert-warning ValidationOK" id="errOrgType">*</span>

                </div>
            </p>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <p>
                <div class="input-group">
                    <div class="col-xs-2" style="padding-left: 0; padding-right: 0; ">
                        <span class="input-group-addon borderMod" style="white-space: pre-wrap;">Company Category</span>
                    </div>
                    <div class="col-xs-10" style="padding-left: 0">
                        <select id="ddlOrgCategory" multiple="multiple" style="width:100% !important">
                            <option value="manufacturer">&nbsp;Manufacturer</option>
                            <option value="trader">&nbsp;Distributor</option>
                            <option value="serviceprovider">&nbsp;Authorized Agent</option>
                            <option value="serviceprovider">&nbsp;Importer</option>
                            <option value="serviceprovider">&nbsp;Sole Agent</option>
                            <option value="serviceprovider">&nbsp;Wholeseller</option>
                        </select>
                    </div>
                    <span class="input-group-addon alert-warning ValidationOK" id="errOrgCategory">*</span>
                </div>
            </p>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Year of Company Establishment:</span>
                <input class="form-control" id="txtTinNo" type="text">
                <span class="input-group-addon alert-warning ValidationOK" id="errTinNo">*</span>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12 well modWell">
            <div class="col-md-12">ADDRESS</div>
        </div>
        <div class="col-md-12">
            <div class="input-group">
                <span class="input-group-addon">Street</span>
                <input class="form-control" id="txtADStreet" type="text" disabled>
            </div>
        </div>
        <div class="col-md-12">
            <div class="">
                <div class="input-group">
                    <span class="input-group-addon">City</span>
                    <input class="form-control" id="txtADStreet" type="text" disabled>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="">
                <div class="input-group">
                    <span class="input-group-addon">Thana</span>
                    <input class="form-control" id="txtADStreet" type="text" disabled>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="">
                <div class="input-group">
                    <span class="input-group-addon">Country</span>
                    <input class="form-control" id="txtADStreet" type="text" disabled>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 well modWell">
            <div class="col-md-12">COMPANY CONTACT</div>
        </div>
